.admin-container {
  padding: 30rpx;
  min-height: 100vh;
  box-sizing: border-box;
  background: #f8fafc;
}
.admin-container .dashboard-cards {
  display: grid;
  grid-template-columns: repeat(2, 1fr);
  gap: 24rpx;
  margin-bottom: 32rpx;
}
.admin-container .dashboard-cards .card {
  padding: 32rpx;
  border-radius: 16rpx;
  color: #fff;
  display: flex;
  flex-direction: column;
  align-items: center;
}
.admin-container .dashboard-cards .card-value {
  font-size: 48rpx;
  font-weight: 700;
  margin: 16rpx 0;
}
.admin-container .dashboard-cards .card-label {
  font-size: 24rpx;
  opacity: 0.9;
}
.admin-container .dashboard-cards .card.total-card {
  background: linear-gradient(135deg, #667eea, #764ba2);
}
.admin-container .dashboard-cards .card.success-card {
  background: linear-gradient(135deg, #48bb78, #38a169);
}
.admin-container .search-bar {
  background: #fff;
  border-radius: 16rpx;
  padding: 24rpx;
  box-shadow: 0 4rpx 12rpx rgba(0, 0, 0, 0.05);
}
.admin-container .search-bar .search-header {
  display: flex;
  align-items: center;
  margin-bottom: 24rpx;
}
.admin-container .search-bar .search-header .section-title {
  font-size: 32rpx;
  font-weight: 600;
  color: #1a365d;
  margin-right: 16rpx;
}
.admin-container .search-bar .filter-section {
  display: flex;
  gap: 24rpx;
  margin-top: 24rpx;
}
.admin-container .search-bar .filter-section .filter-item {
  flex: 1;
  padding: 20rpx;
  background: #f5f7fa;
  border-radius: 12rpx;
  display: flex;
  align-items: center;
  justify-content: center;
  color: #4a5568;
  font-size: 28rpx;
}
.admin-container .search-bar .filter-section .filter-item text {
  margin-left: 12rpx;
}
.admin-container .data-list {
  margin-top: 32rpx;
}
.admin-container .data-list .data-item {
  background: #fff;
  border-radius: 16rpx;
  margin-bottom: 24rpx;
  padding: 24rpx;
  box-shadow: 0 2rpx 8rpx rgba(0, 0, 0, 0.05);
}
.admin-container .data-list .data-item-even {
  background: #fdfdfe;
}
.admin-container .data-list .data-item .item-header {
  display: flex;
  justify-content: space-between;
  align-items: center;
  padding-bottom: 24rpx;
  border-bottom: 1rpx solid #eee;
}
.admin-container .data-list .data-item .item-header .device-info {
  display: flex;
  align-items: center;
}
.admin-container .data-list .data-item .item-header .device-info .device-id {
  font-weight: 600;
  color: #2d3748;
  margin: 0 16rpx;
}
.admin-container .data-list .data-item .item-header .device-info .process-name {
  color: #718096;
  font-size: 26rpx;
}
.admin-container .data-list .data-item .item-header .status-badge {
  padding: 8rpx 24rpx;
  border-radius: 20rpx;
  font-size: 24rpx;
  font-weight: 500;
}
.admin-container .data-list .data-item .item-header .status-badge.status-pass {
  background: #c6f6d5;
  color: #22543d;
}
.admin-container .data-list .data-item .item-header .status-badge.status-fail {
  background: #fed7d7;
  color: #742a2a;
}
.admin-container .data-list .data-item .item-details {
  padding: 24rpx 0;
}
.admin-container .data-list .data-item .item-details .detail-item {
  display: flex;
  align-items: center;
  margin-bottom: 20rpx;
  color: #4a5568;
  font-size: 28rpx;
}
.admin-container .data-list .data-item .item-details .detail-item text {
  margin-left: 12rpx;
}
.admin-container .data-list .data-item .item-details .inspect-params {
  margin-bottom: 24rpx;
}
.admin-container .data-list .data-item .item-details .inspect-params .param-item {
  display: flex;
  align-items: center;
  margin-bottom: 12rpx;
}
.admin-container .data-list .data-item .item-details .inspect-params .param-item .param-label {
  font-size: 26rpx;
  font-weight: 600;
  color: #2d3748;
  margin-right: 12rpx;
}
.admin-container .data-list .data-item .item-details .inspect-params .param-item .param-value {
  font-size: 24rpx;
  color: #718096;
}
.admin-container .data-list .data-item .item-details .production-info {
  display: flex;
  justify-content: space-between;
  align-items: center;
}
.admin-container .data-list .data-item .item-details .production-info .info-item {
  display: flex;
  align-items: center;
  color: #718096;
  font-size: 26rpx;
}
.admin-container .data-list .data-item .item-details .production-info .info-item text {
  margin-left: 12rpx;
}
.admin-container .data-list .data-item .item-details .image-preview {
  margin-top: 24rpx;
}
.admin-container .data-list .data-item .item-details .image-preview .image-grid {
  display: grid;
  grid-template-columns: repeat(3, 1fr);
  gap: 16rpx;
}
.admin-container .data-list .data-item .item-details .image-preview .image-grid .image-item {
  position: relative;
  border-radius: 12rpx;
  overflow: hidden;
  aspect-ratio: 1;
}
.admin-container .data-list .data-item .item-details .image-preview .image-grid .image-item .preview-image {
  width: 100%;
  height: 100%;
}
.admin-container .data-list .data-item .item-details .image-preview .image-grid .image-item .image-hover {
  position: absolute;
  top: 0;
  left: 0;
  right: 0;
  bottom: 0;
  background: rgba(0, 0, 0, 0.3);
  display: flex;
  align-items: center;
  justify-content: center;
  opacity: 0;
  transition: opacity 0.3s;
}
.admin-container .data-list .data-item .item-details .image-preview .image-grid .image-item:hover .image-hover {
  opacity: 1;
}
.admin-container .data-list .data-item .item-actions {
  display: flex;
  justify-content: flex-end;
  gap: 24rpx;
  padding-top: 24rpx;
  border-top: 1rpx solid #eee;
}
.admin-container .data-list .data-item .item-actions .action-btn {
  display: flex;
  align-items: center;
  padding: 12rpx 24rpx;
  border-radius: 8rpx;
  font-size: 26rpx;
  transition: all 0.2s;
}
.admin-container .data-list .data-item .item-actions .action-btn text {
  margin-left: 8rpx;
}
.admin-container .data-list .data-item .item-actions .action-btn.export-btn {
  color: #3a8bcd;
  background: #ebf8ff;
}
.admin-container .data-list .data-item .item-actions .action-btn.export-btn:hover {
  background: #bee3f8;
}
.admin-container .data-list .data-item .item-actions .action-btn.delete-btn {
  color: #e53e3e;
  background: #fff5f5;
}
.admin-container .data-list .data-item .item-actions .action-btn.delete-btn:hover {
  background: #fed7d7;
}
.admin-container .data-list .data-item .advanced-actions {
  display: flex;
  justify-content: flex-end;
  gap: 24rpx;
  padding-top: 24rpx;
  border-top: 1rpx solid #eee;
}
.admin-container .data-list .data-item .advanced-actions .action-btn {
  display: flex;
  align-items: center;
  padding: 12rpx 24rpx;
  border-radius: 8rpx;
  font-size: 26rpx;
  transition: all 0.2s;
}
.admin-container .data-list .data-item .advanced-actions .action-btn text {
  margin-left: 8rpx;
}
.admin-container .data-list .data-item .advanced-actions .action-btn.tag-btn {
  color: #3a8bcd;
  background: #ebf8ff;
}
.admin-container .data-list .data-item .advanced-actions .action-btn.tag-btn:hover {
  background: #bee3f8;
}
.admin-container .data-list .data-item .advanced-actions .action-btn.remark-btn {
  color: #3a8bcd;
  background: #ebf8ff;
}
.admin-container .data-list .data-item .advanced-actions .action-btn.remark-btn:hover {
  background: #bee3f8;
}
.admin-container .data-list .data-item .advanced-actions .action-btn.history-btn {
  color: #3a8bcd;
  background: #ebf8ff;
}
.admin-container .data-list .data-item .advanced-actions .action-btn.history-btn:hover {
  background: #bee3f8;
}
.admin-container .data-list .loading-tip {
  padding: 48rpx 0;
}
.admin-container .export-controls {
  margin: 24rpx 0;
  display: grid;
  gap: 24rpx;
  position: relative;
  z-index: 1;
}
.admin-container .export-controls .export-btn {
  display: flex;
  align-items: center;
  justify-content: center;
  gap: 12rpx;
  padding: 20rpx;
  border-radius: 12rpx;
  font-size: 28rpx;
}
.admin-container .export-controls .export-btn.excel-btn {
  background: #38a169;
  color: #fff;
}
.admin-container .export-controls .export-btn.image-btn {
  background: #3a8bcd;
  color: #fff;
}
.admin-container .export-controls .image-export {
  display: grid;
  gap: 16rpx;
  grid-template-columns: 1fr auto;
}
.admin-container .stat-panels {
  display: grid;
  grid-template-columns: repeat(2, 1fr);
  gap: 24rpx;
  margin-top: 32rpx;
}
.admin-container .stat-panels .panel {
  background: #fff;
  border-radius: 16rpx;
  padding: 24rpx;
  box-shadow: 0 2rpx 8rpx rgba(0, 0, 0, 0.05);
}
.admin-container .stat-panels .panel h3 {
  font-size: 32rpx;
  font-weight: 600;
  color: #1a365d;
  margin-bottom: 24rpx;
}
.admin-container .stat-panels .panel .chart-container {
  height: 300rpx;
}
.admin-container .file-list-section {
  background: #fff;
  border-radius: 16rpx;
  padding: 24rpx;
  margin-top: 24rpx;
}
.admin-container .file-list-section .section-header {
  display: flex;
  align-items: center;
  gap: 16rpx;
  margin-bottom: 24rpx;
}
.admin-container .file-list-section .section-header .section-title {
  font-size: 32rpx;
  font-weight: 600;
}
.admin-container .file-list-section .section-header .update-time {
  margin-left: auto;
  font-size: 24rpx;
  color: #666;
}
.admin-container .file-list-section .file-scroll {
  max-height: 400rpx;
}
.admin-container .file-list-section .file-scroll .file-item {
  display: flex;
  align-items: center;
  padding: 16rpx;
  border-bottom: 1rpx solid #eee;
}
.admin-container .file-list-section .file-scroll .file-item .file-name {
  flex: 2;
  overflow: hidden;
  text-overflow: ellipsis;
  white-space: nowrap;
}
.admin-container .file-list-section .file-scroll .file-item .file-time {
  flex: 1;
  font-size: 24rpx;
  color: #666;
}
.admin-container .file-list-section .file-scroll .file-item .file-status {
  flex: 0 0 120rpx;
  text-align: center;
  padding: 4rpx 12rpx;
  border-radius: 20rpx;
  font-size: 24rpx;
}
.admin-container .file-list-section .file-scroll .file-item .file-status[status="0"] {
  background: #f6e05e;
  color: #744210;
}
.admin-container .file-list-section .file-scroll .file-item .file-status[status="1"] {
  background: #c6f6d5;
  color: #22543d;
}
.admin-container .file-list-section .file-scroll .file-item .file-status[status="2"] {
  background: #fed7d7;
  color: #742a2a;
}
.action-bar {
  display: flex;
  gap: 20rpx;
  padding: 30rpx;
  background: #fff;
}
.action-bar button {
  flex: 1;
  padding: 20rpx;
  border-radius: 10rpx;
}
.action-bar button.excel-btn {
  background: #4CAF50;
  color: white;
}
.action-bar button.image-btn {
  background: #2196F3;
  color: white;
}
.file-list {
  margin: 30rpx;
  background: #fff;
  border-radius: 10rpx;
}
.file-list .file-item {
  display: flex;
  align-items: center;
  padding: 30rpx;
  border-bottom: 1rpx solid #eee;
}
.file-list .file-item .file-name {
  flex: 2;
  overflow: hidden;
  text-overflow: ellipsis;
  white-space: nowrap;
}
.file-list .file-item .file-time {
  flex: 1;
  color: #666;
  font-size: 24rpx;
}
.file-list .file-item .file-actions {
  flex: 1;
  display: flex;
  gap: 10rpx;
}
.file-list .file-item .file-actions button {
  padding: 10rpx 20rpx;
  background: #f5f5f5;
  border-radius: 6rpx;
}